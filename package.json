{
  "name": "conversation-filer",
  "version": "2.0.0",
  "description": "React.js port of the Conversation Filer Agave",
  "main": "./dist/functions.js",
  "files": [
    "./Content/",
    "./dist/",
    "./*.html"
  ],
  "scripts": {
    "build": "webpack --optimize-minimize",
    "start": "webpack-dev-server --open --watch",
    "test": "jest"
  },
  "keywords": [
    "Agave",
    "Outlook"
  ],
  "author": "Bill Avery <wravery@hotmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/wravery/ConversationFiler.git"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js",
      "^.+\\.jsx?$": "babel-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "^21.1.9",
    "@types/jquery": "^3.2.17",
    "@types/office-js": "0.0.50",
    "@types/react": "^16.0.31",
    "@types/react-addons-test-utils": "^0.14.17",
    "@types/react-dom": "^16.0.3",
    "@types/react-test-renderer": "^16.0.0",
    "awesome-typescript-loader": "^3.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.23.0",
    "jest": "^22.0.3",
    "jquery": "^3.2.1",
    "office-ui-fabric-react": "^5.35.0",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0",
    "source-map-loader": "^0.2.0",
    "ts-jest": "^22.0.0",
    "typescript": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.4.2"
  }
}
